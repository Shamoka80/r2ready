  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "test": "npm run test:client && npm run test:server",
    "test:client": "cd client && npm run test",
    "test:server": "cd server && npm run test",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "cd client && npm run lint",
    "lint:server": "cd server && npm run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint:client -- --fix && npm run lint:server -- --fix",
    "type-check": "cd client && npx tsc --noEmit && cd ../server && npx tsc --noEmit",
    "quality-check": "npm run format:check && npm run lint && npm run type-check",
    "validate:templates": "cd server && npx tsx tools/validate-templates.ts",
    "test:e2e": "npx tsx scripts/run-e2e.ts",
    "test:ui": "npx tsx scripts/verify-ui.ts",
    "test:smoke": "npx playwright test tests/ui/smoke.spec.ts",
    "test:visual": "npx playwright test tests/ui/visual.spec.ts",
    "scripts:verify-industry-alignment": "npx tsx scripts/verify-industry-alignment.ts",
    "scripts:verify-phase1-complete": "npx tsx scripts/verify-phase1-complete.ts",
    "start": "node start.js",
    "validate-critical": "tsx scripts/validate-critical-files.ts"
  },
